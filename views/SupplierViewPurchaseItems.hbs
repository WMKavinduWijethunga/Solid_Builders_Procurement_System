<div class="row">
    <div class="col-6">
        <h3>{{oid}}</h3>
    </div>
</div>
<div class="d-flex">
    <div class="dash-background">
        <h3>DashBoard</h3>
        <div class="btn-groups">
            <a href="/supplierpage/{{supID}}" type="button" class="btn btn-primary">View Orders</a>
            <a href="/supviewapprove/{{supID}}" type="button" class="btn btn-primary">View Approved</a>
        </div>
    </div>

    <div class="set-tb-width">
        <table class="table table-striped table-light table-hover">
            <thead class="thead-dark">
                <tr style="color:#3ab67d">
                    <th scope="col">Item Name</th>
                    <th scope="col">Price</th>
                    <th scope="col">Quantity</th>

                </tr>
            </thead>
            <tbody>
                {{#each rows}}
                <tr>
                    <td>{{this.itemName }}</td>
                    <td>{{this.price}}</td>
                    <td>{{this.quantity}}</td>
                </tr>
                {{/each}}

            </tbody>
        </table>

    </div>

</div>
<div class="btn-set-margin-top">
    <a class="btn btn-primary" type="button" href="/getinsertQuotation/{{oid}}/{{supID}}">Send Quotation</a>
</div>