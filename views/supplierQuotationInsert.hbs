{{clearNumber}}
<form method="POST" action="/insertQuotation">
  <table class="table col-6">
    <thead class="thead-dark">
      <tr>
        <th scope="col">Item Name</th>
        <th scope="col">Quantity</th>
        <th scope="col">Price</th>

      </tr>
    </thead>
    <tbody>

      {{#each rows}}
        <tr>

          <td>{{this.itemName}}</td>
          <td><input name="{{getShortComment}}"
              
              type="text"
              class="form-control"
              value="{{this.quantity}}"
            />

          </td>
          <td><input
              type="text"
              class="form-control"
              value="{{this.price}}"
            /></td>
        </tr>
      {{/each}}

    </tbody>
  </table>

  <div class="col-3">
    <input type="date" class="form-control" id="reqDate" name="quotaionDate" />
  </div>
  <div class="col-3">
    <input
      type="text"
      value="{{q_pid}}"
      class="form-control"
      id="purID"
      name="orderID"
    />
  </div>
  <div class="col-3">
    <input type="text" class="form-control" id="supID" name="supplierID" />
  </div>

  <button type="submit" class="btn btn-primary">Submit</button>

  {{getShortComment 10}}
</form>